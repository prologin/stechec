\ProvidesPackageRCS $Header: /cvs/projectwd/appendices/presentation/styles/beamercolorthemeacuskyblue.sty,v 1.1 2005/09/30 11:56:44 avrill_m Exp $

\mode<presentation>

\definecolor{blue-mi}{rgb}{0,0.39,0.78}

\setbeamercolor{palette primary}{use=structure,fg=white,bg=blue-mi}
\setbeamercolor{palette secondary}{use=structure,fg=white,bg=blue-mi}
\setbeamercolor{palette tertiary}{use=structure,fg=white,bg=blue-mi}
\setbeamercolor{palette quaternary}{use=structure,fg=white,bg=blue-mi}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\setbeamercolor{author in head/foot}{fg=white,bg=blue-mi}
\setbeamercolor{date in head/foot}{fg=white,bg=blue-mi}
\setbeamercolor{title in head/foot}{fg=white,bg=blue-mi}
\setbeamercolor{item}{fg=blue,bg=blue}
\setbeamercolor{section in toc}{fg=blue,bg=blue}

\setbeamercolor{block title}{parent=palette secondary}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg!75!black}
\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg!75!black}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!10!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!10!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!10!bg}

\mode
<all>

 
 