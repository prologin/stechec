\ProvidesPackageRCS $Header: /cvs/projectwd/appendices/presentation/styles/beamercolorthemeacuorange.sty,v 1.1 2005/09/30 11:56:44 avrill_m Exp $

\mode<presentation>

\definecolor{orange-clair}{rgb}{1,0.85,0.46}
\definecolor{orange-mi}{rgb}{1,0.78,0.39}

\setbeamercolor{palette primary}{use=structure,fg=black,bg=orange-mi}
\setbeamercolor{palette secondary}{use=structure,fg=black,bg=orange-clair}
\setbeamercolor{palette tertiary}{use=structure,fg=black,bg=orange-mi}
\setbeamercolor{palette quaternary}{use=structure,fg=black,bg=orange}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\setbeamercolor{author in head/foot}{fg=black,bg=orange-clair}
\setbeamercolor{date in head/foot}{fg=black,bg=orange-clair}
\setbeamercolor{title in head/foot}{fg=black,bg=orange-clair}
\setbeamercolor{item}{fg=orange,bg=orange}
\setbeamercolor{section in toc}{fg=orange,bg=orange}

\setbeamercolor{block title}{parent=palette secondary}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg!75!black}
\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg!75!black}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!10!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!10!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!10!bg}

\mode
<all>

 
 